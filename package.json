{
  "name": "mygithubportfolio",
  "version": "1.0.0",
  "description": "This is page wher you can see all my Git Hub project summary.",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/icon_ecomerce.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dekstryn/MyGithubPortfolio.git"
  },
  "keywords": [
    "grid",
    "github",
    "css",
    "scss",
    "portfolio",
    "project"
  ],
  "author": "Krzysztof Nyrek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dekstryn/MyGithubPortfolio/issues"
  },
  "homepage": "https://github.com/Dekstryn/MyGithubPortfolio#readme",
  "dependencies": {
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "concat": "^1.0.3",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3"
  }
}
